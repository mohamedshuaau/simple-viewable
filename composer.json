{
    "name": "mohamedshuaau/simple-viewable",
    "description": "A simple Laravel package which records eloquent model views",
    "type": "Library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "counter",
        "counts",
        "views",
        "model"
    ],
    "authors": [
        {
            "name": "Mohamed Shuaau",
            "email": "mohammedshuaau@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/session": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Shuaau\\SimpleViewable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shuaau\\SimpleViewable\\SimpleViewableServiceProvider"
            ],
            "aliases": {
                "SimpleViewable": "Shuaau\\SimpleViewable\\Facades\\SimpleViewable"
            }
        }
    }
}
